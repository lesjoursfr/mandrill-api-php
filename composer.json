{
	"name": "lesjoursfr/mandrill-api-php",
	"type": "library",
	"version": "2.1.1",
	"description": "API client library for the Mandrill email as a service platform",
	"keywords": [
		"email",
		"api"
	],
	"homepage": "https://github.com/lesjoursfr/mandrill-api-php",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "Adrien ERAUD",
			"email": "a.eraud@lesjours.fr",
			"homepage": "https://github.com/e-adrien",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.4 || ^8.0"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.7.2",
		"escapestudios/symfony2-coding-standard": "^3.13.0"
	},
	"autoload": {
		"psr-4": {
			"Mandrill\\": "src/"
		}
	},
	"archive": {
		"exclude": [
			".github",
			".vscode",
			".gitignore",
			"vendor",
			"composer.lock",
			"composer.phar",
			"phpDocumentor.phar",
			".phpdoc",
			".editorconfig",
			".prettierrc.js",
			".php-cs-fixer.php",
			"phpcs.xml",
			"phpcs-report.xml"
		]
	}
}
